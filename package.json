{
  "name": "netflix-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider PORT=3000 next dev",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider next build",
    "start": "cross-env PORT=3000 next start",
    "lint": "next lint && stylelint \"styles/**/*.scss\"",
    "prepare": "husky install",
    "postinstall": "node ./scripts/setup-pdf-worker.mjs",
    "seed:local": "node --experimental-fetch ./scripts/seedLocalContent.mjs",
    "start:mac": "bash ./scripts/start-nextflix-mac.command",
    "start:win": "powershell -ExecutionPolicy Bypass -File ./scripts/start-nextflix-win.ps1",
    "autostart:mac": "bash ./scripts/install-autostart-mac.command",
    "autostart:win": "powershell -ExecutionPolicy Bypass -File ./scripts/install-autostart-win.ps1"
  },
  "dependencies": {
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "axios": "^0.21.1",
    "env-var": "^7.0.1",
    "framer-motion": "^4.1.17",
    "next": "11.0.1",
    "pdfjs-dist": "^3.11.174",
    "react": "17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "sass": "^1.35.2",
    "swiper": "^6.8.0"
  },
  "devDependencies": {
    "@types/react": "17.0.14",
    "@types/react-dom": "^17.0.9",
    "eslint": "7.31.0",
    "eslint-config-next": "11.0.1",
    "husky": "^8.0.0",
    "prettier": "^2.3.2",
    "stylelint": "^16.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "typescript": "4.3.5",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=16 <21"
  }
}
